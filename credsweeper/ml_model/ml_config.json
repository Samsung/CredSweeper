{
    "char_set": "\u001b\t\n\r !\"#$%&'()*+,-./0123456789:;<=>?@ABCDEFGHIJKLMNOPQRSTUVWXYZ[\\]^_`abcdefghijklmnopqrstuvwxyz{|}~",
    "thresholds": {
        "lowest": 0.22917,
        "low": 0.35739,
        "medium": 0.62204,
        "high": 0.79791,
        "highest": 0.92996
    },
    "features": [
        {
            "type": "RuleSeverity",
            "comment": "INFO=0.0, LOW=0.25, MEDIUM=0.5, HIGH=0.75, CRITICAL=1.0",
